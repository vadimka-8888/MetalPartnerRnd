{
  "timestamp": { 
    "$resolver": "timestamp",
    "pattern": {
      "format": "yyyy-MM-dd'T'HH:mm:ss.SSS'Z'",
      "timeZone": "UTC"
    }
  },
  "logger": {
    "$resolver": "logger",
    "field": "name"
  },
  "level": {
    "$resolver": "level",
    "field": "name"
  }, 
  "message": { 
    "$resolver": "message",
    "stringified": true
  }
}

    